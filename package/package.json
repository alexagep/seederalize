{
  "name": "seederalize",
  "version": "0.7.0",
  "description": "Generates seeder file for sequelize from existing databases",
  "main": "./src/server.js",
  "bin": {
    "seederalize": "./bin/seederalize"
  },
  "scripts": {
    "lint": "eslint",
    "test": "npm run test",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "format": "prettier --write \"src/**/*.js\"",
    "db:init": "node -r esm -e 'require(\"./db/init\").initDatabase()'",
    "db:drop": "node -r esm -e 'require(\"./db/drop\").dropDatabase()'",
    "db:migrate": "node -r esm ./node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:undo": "node -r esm ./node_modules/.bin/sequelize-cli db:migrate:undo",
    "db:run:seed": "node -r esm ./node_modules/.bin/sequelize-cli db:seed:all",
    "db:undo:seed": "node -r esm ./node_modules/.bin/sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "Ali_Aghapour @alexagep",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexagep/seederalize"
  },
  "bugs": {
    "url": "https://github.com/alexagep/seederalize.git/issues"
  },
  "homepage": "https://github.com/alexagep/seederalize#readme",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "*",
    "ioredis": "^5.3.0",
    "mysql2": "^3.1.2",
    "pg": "^8.9.0",
    "sequelize": "^6.28.0",
    "sequelize-cli": "*",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "eslint": "^8.35.0",
    "esm": "3.2.25",
    "jest": "^29.4.3",
    "prettier": "^2.8.4"
  }
}
